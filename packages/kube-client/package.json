{
  "name": "@gardener-dashboard/kube-client",
  "version": "1.0.0",
  "description": "Kubernetes Client",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gardener/dashboard.git",
    "directory": "packages/kube-client"
  },
  "bugs": {
    "url": "https://github.com/gardener/dashboard/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint --ext .js .",
    "test": "jest",
    "test-coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@gardener-dashboard/kube-config": "workspace:*",
    "@gardener-dashboard/logger": "workspace:*",
    "@gardener-dashboard/request": "workspace:*",
    "agentkeepalive": "^4.1.0",
    "base64url": "^3.0.1",
    "delay": "^4.3.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mixwith": "^0.1.1",
    "moment": "^2.24.0",
    "node-forge": "^0.10.0",
    "p-event": "^4.1.0",
    "p-retry": "^4.2.0",
    "p-timeout": "^3.2.0",
    "reconnect-core": "^1.3.0",
    "uuid": "^8.3.0",
    "ws": "^7.0.1"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "jest": "^26.4.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es2020": true,
      "node": true,
      "jest/globals": true
    },
    "plugins": [
      "jest"
    ],
    "extends": [
      "standard",
      "plugin:jest/recommended"
    ],
    "globals": {},
    "parserOptions": {
      "ecmaVersion": 11
    },
    "rules": {
      "no-console": "error"
    }
  },
  "jest": {
    "restoreMocks": true,
    "verbose": true,
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 95,
        "lines": 92,
        "statements": 92
      }
    }
  }
}
