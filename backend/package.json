{
  "name": "@gardener-dashboard/backend",
  "version": "1.79.0",
  "description": "Gardener Dashboard Server",
  "license": "Apache-2.0",
  "author": "Koser, Holger <holger.koser@sap.com>",
  "contributors": [
    "Herz, Andreas <andreas.herz@sap.com>",
    "Sutter, Peter <peter.sutter@sap.com>",
    "Gross, Lukas <lukas.gross@sap.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gardener/dashboard.git",
    "directory": "backend"
  },
  "bugs": {
    "url": "https://github.com/gardener/dashboard/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "server.mjs",
  "files": [
    "lib",
    "server.mjs"
  ],
  "scripts": {
    "serve": "cross-env NODE_ENV=development node server.mjs",
    "lint": "eslint server.mjs . --max-warnings=0",
    "lint-sarif": "yarn lint --format=@microsoft/eslint-formatter-sarif",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules $(yarn bin jest)",
    "test-coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@gardener-dashboard/kube-client": "workspace:^",
    "@gardener-dashboard/kube-config": "workspace:^",
    "@gardener-dashboard/logger": "workspace:^",
    "@gardener-dashboard/monitor": "workspace:^",
    "@gardener-dashboard/request": "workspace:^",
    "@godaddy/terminus": "^4.12.1",
    "@octokit/auth-app": "^6.0.4",
    "@octokit/core": "^5.1.0",
    "@octokit/plugin-paginate-graphql": "^4.0.1",
    "@octokit/plugin-paginate-rest": "^9.2.1",
    "@octokit/plugin-request-log": "^4.0.1",
    "@octokit/plugin-rest-endpoint-methods": "^10.4.1",
    "base64url": "^3.0.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.3",
    "express-static-gzip": "^2.1.7",
    "fnv-plus": "^1.3.1",
    "helmet": "^8.0.0",
    "http-errors": "^2.0.0",
    "jose": "^5.2.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "object-hash": "^3.0.0",
    "openapi-types": "^12.1.3",
    "openid-client": "^6.1.7",
    "p-limit": "3.1.0",
    "p-retry": "4.6.2",
    "p-timeout": "^4.1.0",
    "sanitize-html": "^2.12.1",
    "semver": "^7.6.0",
    "showdown": "^2.1.0",
    "socket.io": "^4.7.5",
    "undici": "^7.3.0",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@gardener-dashboard/test-utils": "workspace:^",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "abort-controller": "^3.0.0",
    "cross-env": "^7.0.3",
    "dockerfile-ast": "^0.7.0",
    "eslint": "^9.11.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-security": "^3.0.1",
    "fast-json-patch": "^3.1.1",
    "jest": "^29.7.0",
    "neostandard": "^0.12.0",
    "p-event": "^4.2.0",
    "path-to-regexp": "^8.0.0",
    "set-cookie-parser": "^2.6.0",
    "socket.io-client": "^4.7.5",
    "supertest": "^7.0.0"
  },
  "jest": {
    "restoreMocks": true,
    "verbose": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "lib/**/*.js",
      "lib/**/*.mjs"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/test-ignore/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "\\.pnp\\.[^\\/]+$"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 68,
        "functions": 94,
        "lines": 90,
        "statements": 90
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "packageManager": "yarn@4.6.0",
  "engines": {
    "node": "^22.7.0"
  }
}
